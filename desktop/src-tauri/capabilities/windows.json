{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "windows-capability",
  "description": "Capability for Windows platform",
  "platforms": ["windows"],
  "windows": ["main", "overlay"],
  "permissions": [
    "core:default",
    "http:default",
    "http:allow-fetch",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send",
    "dialog:default",
    "fs:default",
    "shell:allow-open",
    "store:default",
    "process:default",
    "process:allow-restart"
  ]
}