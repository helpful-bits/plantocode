{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "http:default",
    "http:allow-fetch",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send",
    "stronghold:default",
    "deep-link:default",
    "deep-link:allow-register",
    "dialog:default",
    "fs:default",
    "shell:default",
    "shell:allow-open"
  ]
}
