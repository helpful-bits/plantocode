{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "macos-capability",
  "description": "Capability for macOS platform",
  "platforms": ["macOS"],
  "windows": ["main", "overlay"],
  "permissions": [
    "core:default",
    "http:default",
    "http:allow-fetch",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send",
    "dialog:default",
    "fs:default",
    "shell:allow-open",
    "store:default",
    "updater:default",
    "process:default",
    "process:allow-restart"
  ]
}