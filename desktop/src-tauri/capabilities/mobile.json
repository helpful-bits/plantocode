{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile",
  "description": "Mobile capability for iOS and Android",
  "windows": ["main"],
  "platforms": ["iOS", "android"],
  "permissions": [
    "core:default",
    "core:event:default",
    "http:default",
    "http:allow-fetch",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send",
    "dialog:default",
    "fs:default",
    "store:default",
    "os:default"
  ]
}